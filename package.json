{
  "name": "ride-my-way",
  "version": "1.0.0",
  "description": "A carpooling application that provides drivers with the ability to create ride offers and passengers to join available ride offers",
  "main": "app.js",
  "scripts": {
    "start": "babel-node app.js",
    "start:dev": "nodemon --watch server --exec babel-node app.js",
    "test": "nyc mocha server/tests/routes/*.test.js --exit --compilers js:babel-register && nyc report",
    "test2": "mocha server/tests/routes/*.test.js --exit --compilers js:babel-register",
    "pretest": "cross-env NODE_ENV=test npm run db-migrate",
    "db-migrate": "babel-node ./server/models/index.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-beloved/Ride-my-way.git"
  },
  "keywords": [
    "Node",
    "Express",
    "PostgresSql"
  ],
  "author": "D-beloved",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/d-beloved/Ride-my-way/issues"
  },
  "homepage": "https://github.com/d-beloved/Ride-my-way#readme",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "morgan": "^1.9.0",
    "pg": "^7.4.3",
    "validator": "^10.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.17.5",
    "nyc": "^12.0.2",
    "supertest": "^3.1.0"
  }
}
